<Window x:Class="ADBFastbootGUI.Windows.ProgramsListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADBFastbootGUI.Windows"
        mc:Ignorable="d"
        WindowStyle="None"
        ShowInTaskbar="False"
        Loaded="Window_Loaded"
        WindowStartupLocation="CenterOwner"
        Title="ProgramsListWindow" Height="508.3" Width="522.875">

    <WindowChrome.WindowChrome>
        <WindowChrome GlassFrameThickness="0" ResizeBorderThickness="0" CaptionHeight="0" CornerRadius="20"/>
    </WindowChrome.WindowChrome>
    
    <Grid>
        <TextBox x:Name="PackagesBox" IsReadOnly="True" Text="" HorizontalContentAlignment="Center" VerticalScrollBarVisibility="Visible" Margin="0,0,0,50"/>
        <TextBlock x:Name="Title" Text="Packages" FontSize="20" HorizontalAlignment="Center"/>
        <Button x:Name="CancelButton" Content="Cancel" Width="94" Height="30" BorderThickness="0" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,10,10" FontSize="17" Click="CancelButton_Click"/>
        <Button x:Name="ExprortPackageIDsButton" Content="Export" Width="94" Height="30" BorderThickness="0" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,121,10" FontSize="17" Click="ExportPackageIDsButton_Click"/>
    </Grid>
</Window>
